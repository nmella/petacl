<config>
    <modules>
        <Ophirah_Fakepro>
            <version>1.0.5</version>
        </Ophirah_Fakepro>
    </modules>
    <global>
        <models>
            <fakepro>
                <class>Ophirah_Fakepro_Model</class>
            </fakepro>
        </models>
        <helpers>
            <fakepro>
                <class>Ophirah_Fakepro_Helper</class>
            </fakepro>
        </helpers>
        <blocks>
            <fakepro>
                <class>Ophirah_Fakepro_Block</class>
            </fakepro>
        </blocks>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <ophirah_fakepro>
                    <file>fakepro.xml</file>
                </ophirah_fakepro>
            </updates>
        </layout>
    </adminhtml>
    <frontend>
        <layout>
            <updates>
                <ophirah_fakepro>
                    <file>fakepro.xml</file>
                </ophirah_fakepro>
            </updates>
        </layout>
    </frontend>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <fakepro before="Mage_Adminhtml">Ophirah_Fakepro_Adminhtml</fakepro>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
