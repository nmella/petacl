<layout>
 	<default>
        <reference name="head">
            <action method="addJs"><script>moogento/pickpack/adminhtml/grouping.js</script></action>
        </reference>
    </default>
    <adminhtml_system_config_edit>
        <reference name="head">
            <action method="addJs"><script>moogento/jscolor/jscolor.js</script></action>
            <!-- add css vs js for config section here -->
            <action  method="addCss"><name>moogento/general/config.css</name></action>
		</reference>    
    </adminhtml_system_config_edit>
	<handle_add_css_js>
		<reference name="head">
			<action method="addCss"><name>moogento/pickpack/pickpack.css</name></action>
		</reference>
	</handle_add_css_js>
</layout>
